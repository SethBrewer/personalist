<h1>New Myers Briggs type</h1>

<% form_for(@myers_briggs_type) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :mbti_type_name %><br />
    <%= f.text_field :mbti_type_name %>
  </p>

  <p>
    <%= f.label :mbti_type_description %><br />
    <%= f.text_area :mbti_type_description %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', myers_briggs_types_path %>
